{
  "name": "conquery",
  "version": "0.1.0",
  "description": "conquery",
  "main": "server.js",
  "private": true,
  "engines": {
    "node": ">=6.5.0 <7.0.0",
    "yarn": ">=0.16.1"
  },
  "scripts": {
    "start": "node server",
    "start-production": "NODE_ENV=production PORT=8000 node server > webapp.log 2> webapp-err.log &",
    "build": "rimraf dist && NODE_ENV=production webpack --config ./webpack.production.config.js --progress --profile --colors",
    "lint": "eslint --ext=js src/js test",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "test": "rimraf coverage && nyc _mocha test",
    "test-watch": "mocha test --watch",
    "debug-test": "node-debug --debug-brk --hidden node_modules _mocha test"
  },
  "dependencies": {
    "autoprefixer": "^6.7.6",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.24.0",
    "classnames": "^2.2.5",
    "create-react-class": "^15.5.3",
    "css-loader": "^0.27.0",
    "empty": "^0.10.1",
    "es6-promise": "^3.2.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "flow-bin": "^0.52.0",
    "font-awesome": "^4.7.0",
    "font-awesome-webpack": "^0.0.5-beta.2",
    "html-webpack-plugin": "^2.28.0",
    "i18n-react": "^0.3.0-ts1",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "moment": "^2.17.0",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.3.3",
    "prop-types": "^15.5.9",
    "query-string": "^5.0.1",
    "raw-loader": "^0.5.1",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.3.1",
    "react-datepicker": "0.32.0",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^15.4.2",
    "react-dropzone": "^3.12.2",
    "react-input-autosize": "^1.1.3",
    "react-list": "^0.8.0",
    "react-onclickoutside": "^5.7.1",
    "react-redux": "^5.0.2",
    "react-router": "^4.2.0",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-select": "^1.0.0-rc.4",
    "react-split-pane": "^0.1.57",
    "react-tooltip": "^3.2.9",
    "redux": "^3.6.0",
    "redux-form": "^7.0.3",
    "redux-multi": "^0.1.12",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^6.0.3",
    "stats-webpack-plugin": "^0.5.0",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "yaml-loader": "^0.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.1",
    "body-parser": "^1.17.2",
    "chai": "^3.5.0",
    "dirty-chai": "^1.2.2",
    "eslint": "^3.18.0",
    "eslint-config-standard": "^7.1.0",
    "eslint-config-standard-react": "^4.3.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-mocha": "^4.9.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.5.0",
    "eslint-plugin-standard": "^2.0.1",
    "expect": "^1.20.2",
    "flow-typed": "^2.1.5",
    "flux-standard-action": "^0.6.1",
    "ghooks": "^1.3.2",
    "jsdom": "^11.3.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^3.1.2",
    "mock-local-storage": "^1.0.5",
    "nock": "^8.0.0",
    "nyc": "^9.0.1",
    "redux-logger": "^2.8.1",
    "redux-mock-store": "^1.2.0",
    "timekeeper": "^0.1.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1"
  },
  "config": {
    "ghooks": {
      "pre-push": "npm run lint"
    }
  },
  "nyc": {
    "extension": [
      ".jsx"
    ],
    "exclude": [
      "node_modules",
      "test/**/*.js"
    ],
    "reporter": [
      "text",
      "lcov"
    ]
  }
}
